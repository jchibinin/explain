{
    "manifest_version": 2,
    "name": "Describe!",
    "description":"This extension helps users describe things.",
    "version": "1.0",
    "options_page": "options.html",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
}
  